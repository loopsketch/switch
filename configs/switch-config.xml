<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE switch-config SYSTEM "switch-config.dtd"[]>
<config>
	<!-- ログ設定 -->
	<log>
		<file>switch.log</file>
		<pattern>%Y-%m-%d %H:%M:%S.%c %N[%T]:%t</pattern>
		<archive>timestamp</archive>
		<compress>true</compress>
		<rotation>daily</rotation>
		<purgeage>5days</purgeage>
	</log>
	<!-- 画面のサイズを設定 -->
	<display>
		<width>1024</width>
		<height>768</height>
		<rate>60</rate>
		<fullscreen>false</fullscreen>
		<frame>false</frame>
	</display>
	<stage>
		<x>0</x>
		<y>0</y>
		<width>640</width>
		<height>480</height>
		<luminnace>100</luminnace>
	</stage>
	<!--
	<stage width="2400" height="120">
		<imageSplitWidth>2400</imageSplitWidth>
		<textHeight>80</textHeight>
	<luminnace>100</luminnace>
	</stage>
	<split h="120" type="vertical-up" w="80"/>
-->
  <!-- ワークスペース設定ファイル名を指定 -->
  <scenes>main,operation</scenes>
  <!-- ワークスペース設定ファイル名を指定 -->
  <workspace>workspace.xml</workspace>

</config>